{
  "private": true,
  "name": "basic-framework-web",
  "scripts": {
    "start": "cross-env BROWSER=none PORT=4000 roadhog dev",
    "watch": "COMPRESS=none NODE_ENV=development roadhog build --watch",
    "egg": "cross-env NODE_ENV=development egg-bin dev --port=8888 --daemon --title=basic-framework-web",
    "server-start": "egg-scripts start --port=9999 --daemon --env=prod --title=basic-framework-web",
    "server-stop": "egg-scripts stop --title=basic-framework-web",
    "server-start-stag": "egg-scripts start --port=9998 --daemon --env=stag --title=basic-framework-web",
    "server-start-local": "egg-scripts start --port=9997 --env=local --title=basic-framework-web",
    "build-dev": "COMPRESS=none roadhog build",
    "build": "ESLINT=none roadhog build",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint",
    "debug": "egg-bin dev --debug $NODE_DEBUG_OPTION"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "~3.15.2",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.8.0",
    "brace": "^0.11.1",
    "braft-editor": "^2.3.8",
    "browser-cookies": "^1.2.0",
    "cross-env": "^6.0.3",
    "dva": "^2.4.0",
    "egg": "~2.16.0",
    "egg-cors": "^2.1.0",
    "egg-scripts": "^2.9.1",
    "egg-static": "^2.1.1",
    "egg-view": "^2.1.0",
    "egg-view-art": "^1.0.3",
    "history": "~4.9.0",
    "immer": "^5.0.0",
    "koa-compress": "^3.0.0",
    "koa-proxies": "^0.9.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-ace": "^8.0.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.2.0",
    "roadhog": "^2.5.0-beta.4"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dva-hmr": "^0.3.2",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-umi": "^0.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "husky": "^0.12.0",
    "redbox-react": "^1.4.3",
    "svg-inline-loader": "^0.8.0",
    "svg-sprite-loader": "^4.1.1"
  }
